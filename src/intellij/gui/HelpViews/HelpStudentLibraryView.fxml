<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label text="What is this page?" />
        <Label prefWidth="500.0" text="The library page displays the available books you can check out." wrapText="true">
            <VBox.margin>
                <Insets top="5.0" />
            </VBox.margin>
        </Label>
        <Label layoutX="10.0" layoutY="10.0" text="How to check out a book?">
            <VBox.margin>
                <Insets top="20.0" />
            </VBox.margin>
        </Label>
        <Label layoutX="10.0" layoutY="32.0" prefWidth="500.0" text="Select the book you would like to check out and press check out." wrapText="true">
            <VBox.margin>
                <Insets top="5.0" />
            </VBox.margin>
        </Label>
        <Label layoutX="10.0" layoutY="86.0" text="Is there a limit to how many books per user">
            <VBox.margin>
                <Insets top="20.0" />
            </VBox.margin>
        </Label>
        <Label layoutX="10.0" layoutY="108.0" prefWidth="500.0" text="Yes, you can only have 5 books checked out." wrapText="true">
            <VBox.margin>
                <Insets top="5.0" />
            </VBox.margin>
        </Label>
    </children>
    <padding>
        <Insets bottom="50.0" left="50.0" right="50.0" top="50.0" />
    </padding>
</VBox>
